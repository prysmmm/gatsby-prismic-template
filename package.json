{
  "name": "template",
  "version": "1.0.0",
  "private": true,
  "description": "Gatsby Prismic template",
  "author": "@arnauddecock",
  "scripts": {
    "postinstall": "husky install && rm -rf .git/hooks && ln -s ../.husky .git/hooks",
    "develop": "gatsby develop",
    "develop:https": "gatsby develop --https",
    "build": "yarn type:check && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "prettier --check .",
    "format": "prettier --write .",
    "noIndex": "yarn build && echo 'User-agent: *\nDisallow: /' > public/robots.txt",
    "type:check": "tsc"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@chakra-ui/gatsby-plugin": "^3.0.6",
    "@chakra-ui/icons": "^2.0.11",
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-brands-svg-icons": "^6.2.0",
    "@fortawesome/pro-light-svg-icons": "^6.2.0",
    "@fortawesome/pro-regular-svg-icons": "^6.2.0",
    "@fortawesome/pro-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/error-message": "^2.0.0",
    "dayjs": "^1.11.6",
    "flat": "^5.0.2",
    "framer-motion": "^7.6.1",
    "gatsby": "^4.24.4",
    "gatsby-plugin-image": "2.24.0",
    "gatsby-plugin-manifest": "^4.24.0",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-polyfill-io": "^1.1.0",
    "gatsby-plugin-prismic-previews": "^5.2.10",
    "gatsby-plugin-react-svg": "^3.1.0",
    "gatsby-plugin-robots-txt": "^1.7.1",
    "gatsby-plugin-sharp": "^4.24.0",
    "gatsby-plugin-sitemap": "^5.24.0",
    "gatsby-plugin-typescript": "^4.24.0",
    "gatsby-source-filesystem": "^4.24.0",
    "gatsby-source-prismic": "^5.2.10",
    "gatsby-transformer-sharp": "^4.24.0",
    "i18next": "21.10.0",
    "node-fetch": "2.6.7",
    "prismic-reactjs": "^1.3.4",
    "react": "^18.2.0",
    "react-children-utilities": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-i18next": "11.18.6",
    "react-intersection-observer": "^9.4.0",
    "react-scroll-parallax": "^3.3.1",
    "reading-time": "^1.5.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@commitlint/config-conventional": "^17.1.0",
    "@graphql-codegen/add": "^3.2.1",
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@graphql-codegen/typescript": "2.7.5",
    "@graphql-codegen/typescript-operations": "2.5.5",
    "@graphql-codegen/typescript-react-apollo": "3.3.5",
    "@graphql-tools/graphql-file-loader": "^7.5.5",
    "@graphql-tools/load": "^7.7.7",
    "@graphql-tools/load-files": "^6.6.1",
    "@graphql-tools/merge": "^8.3.6",
    "@graphql-tools/schema": "^9.0.4",
    "eslint": "8.22.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql-import": "^1.0.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1"
  }
}
